--Primeira Query
SELECT 	p.nome AS PAIS,
		re.ano AS ANO,
		ie.nome AS INTERACAO,
		e.tipo AS TIPO_ENERGIA,
		re.valor AS VALOR
FROM RELATORIO_ENERGETICO re
INNER JOIN PAIS p
	ON re.codigo_pais = p.codigo
INNER JOIN INTERACAO_ENERGETICA ie
	ON re.id_interacao = ie.id_interacao
INNER JOIN ENERGIA e
	ON re.id_energia = e.id_energia;


SELECT 	p.nome AS PAIS,
		re.ano AS ANO,
		ie.nome AS INTERACAO,
		e.tipo AS TIPO_ENERGIA,
		SUM(re.valor) AS TOTAL
FROM RELATORIO_ENERGETICO re
INNER JOIN PAIS p
	ON re.codigo_pais = p.codigo
INNER JOIN INTERACAO_ENERGETICA ie
	ON re.id_interacao = ie.id_interacao
INNER JOIN ENERGIA e
	ON re.id_energia = e.id_energia
WHERE e.tipo = 'Wind' OR
GROUP BY p.nome, re.ano, ie.nome, e.tipo
ORDER BY p.nome, re.ano;